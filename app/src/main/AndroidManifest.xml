<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.senai.stdevelopment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SobreNosActivity"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HistoriaActivity"
            android:label="@string/title_activity_historia"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".TelaPrincipalActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MensagemActivity"
            android:label="@string/title_activity_mensagem"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ContatoActivity"
            android:label="@string/title_activity_contato"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>